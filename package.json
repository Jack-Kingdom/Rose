{
  "name": "ownblog",
  "version": "0.0.1",
  "description": "my own blog app",
  "main": "src/backend/index.js",
  "scripts": {
    "dev": "nodemon src/backend/index.js --exec \"babel-node\"",
    "webpack": "nodemon --watch webpack.config.js --exec \"webpack --watch\"",
    "build": "cross-env NODE_ENV=production babel src/backend -d build/backend --source-maps && webpack",
    "serve": "cross-env NODE_ENV=production node build/backend/index.js",
    "update-schema": "babel-node src/utils/updateSchema.js",
    "schema-json": "babel-node src/utils/schemaJson.js",
    "relay": "relay-compiler --src ./src/frontend --schema ./graphql.schema.json"
  },
  "author": "Jack",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-relay": "^1.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "graphql": "^0.10.5",
    "html-webpack-plugin": "^2.29.0",
    "katex": "^0.7.1",
    "medium-editor": "^5.23.1",
    "mongodb": "^2.2.30",
    "mongoose": "^4.11.3",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-relay": "^1.1.0",
    "react-router-dom": "^4.1.2",
    "relay-compiler": "^1.1.0",
    "relay-runtime": "^1.1.0",
    "style-loader": "^0.16.1",
    "uglify-js": "^2.8.29",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "winston": "^2.3.1"
  }
}
