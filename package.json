{
  "name": "ownblog",
  "version": "0.0.1",
  "description": "my own blog app",
  "main": "src/backend/index.js",
  "scripts": {
    "backend": "nodemon src/backend/index.js --exec \"babel-node\"",
    "frontend": "nodemon --watch webpack.config.js --exec \"webpack --watch\"",
    "schema": "nodemon src/utils/updateSchema.js --exec \"babel-node\"",
    "build": "babel src/backend -d build/backend --source-maps && webpack",
    "serve": "node build/backend/index.js",
    "relay": "relay-compiler --src ./src/frontend/application/home/article/src --schema ./src/frontend/application/home/article/article.graphql"
  },
  "author": "Jack",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bluebird": "^3.5.0",
    "css-loader": "^0.28.0",
    "express": "^4.15.2",
    "express-graphql": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "graphql": "^0.9.2",
    "html-webpack-plugin": "^2.28.0",
    "katex": "^0.7.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "medium-editor": "^5.23.0",
    "mongodb": "^2.2.26",
    "mongoose": "^4.9.5",
    "mongorito": "^2.2.0",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "radium": "^0.18.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-relay": "dev",
    "react-router-dom": "^4.1.0",
    "relay": "^0.8.0-1",
    "relay-runtime": "dev",
    "style-loader": "^0.16.1",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "devDependencies": {
    "babel-plugin-relay": "dev",
    "relay-compiler": "dev"
  }
}
